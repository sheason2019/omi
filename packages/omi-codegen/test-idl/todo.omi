struct Todo {
  int id;
  string content;
  boolean finish;
  int createTime;
  TodoType todoType;
}

// block
service ITodo {
  repeated Todo GetTodoList();
  void PostTodo(string content);
  void PutTodo(Todo todo);
  void DeleteTodo(int id);
  void Login();
}

enum TodoType {
  Normal,
  Topping,
}