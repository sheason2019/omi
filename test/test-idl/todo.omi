struct Todo {
  int id;
  string content;
  boolean finish;
  int createTime;
  Todo todoType;
  repeated Todo ChildTodo;
}

struct DeleteTodoPayload {
  int id;
}

// block
service ITodo {
  Todo GetTodoList();
  void PostTodo(Todo todo);
  void PutTodo(Todo todo);
  void DeleteTodo(DeleteTodoPayload payload);
  void Login();
}
